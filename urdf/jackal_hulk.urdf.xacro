<?xml version="1.0"?>
<robot name="jackal_hulk" xmlns:xacro="http://ros.org/wiki/xacro">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

 <xacro:macro name="default_inertial" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </xacro:macro>

   <xacro:macro name="sphere_inertia" params ="m r">
  <inertial>
   <mass value="${m}"/>
   <inertia ixx="${m*(r*r*2)/5}"  ixy="0.0" ixz="0.0"
            iyy="${m*(r*r*2)/5}"  iyz= "0.0"
            izz="${m*(r*r*2)/5}"/>
   </inertial>
  </xacro:macro>
  
  <xacro:macro name="cylinder_inertia" params ="m r h">
  <inertial>
   <mass value="${m}"/>
   <inertia ixx="${m*(3*r*r+h*h)/12}"  ixy="0.0" ixz="0.0"
            iyy="${m*(3*r*r+h*h)/12}"  iyz= "0.0"
            izz="${m*r*r/2}"/>
   </inertial>
  </xacro:macro>

<link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://jackal/meshes/base_link.dae"/>
      </geometry>
      <origin  rpy="0 0 0" xyz="0 0 0" />
      <material name="blue"/>
     </visual>
    <collision>
      <origin  rpy="0 0 0" xyz="0 0 0"  />
      <geometry>
        <mesh filename="package://jackal/meshes/base_link.dae"/>
      </geometry>
    </collision>
    <!--xacro:default_inertial mass="10"/-->
</link>

<gazebo reference="base_link">
    <static>0</static>
    <mu1>1</mu1>
    <mu2>1</mu2>
    <selfCollide>true</selfCollide> 
    <material>Gazebo/Grey</material>
</gazebo>

<link name="link_1">
    <visual>
      <geometry>
        <mesh filename="package://jackal/meshes/link_1.dae"/>
      </geometry>
      <origin  rpy="0 0 0" xyz="0 0 0" />
      <material name="blue"/>
     </visual>
    <collision>
      <origin  rpy="0 0 0" xyz="0 0 0"  />
      <geometry>
       <mesh filename="package://jackal/meshes/link_1.dae"/>
      </geometry>
    </collision>
    <xacro:default_inertial mass="0.1"/>
</link>

<gazebo reference="link_1">
    <static>0</static>
    <mu1>1</mu1>
    <mu2>1</mu2>
    <selfCollide>true</selfCollide> 
    <material>Gazebo/Yellow</material>
</gazebo>


<joint name="joint_1" type="fixed">
      <axis xyz="0 0 0"/> 
      <parent link="base_link"/>
      <child link="link_1"/>
      <origin xyz="0 0 -0.05"/>
</joint>


<link name="link_rf">
    <visual>
      <geometry>
        <mesh filename="package://jackal/meshes/link_2.dae"/>
      </geometry>
      <origin  rpy="0 0 0" xyz="0 0 0" />
      <material name="blue"/>
     </visual>
    <collision>
      <origin  rpy="0 0 0" xyz="0 0 0"  />
      <geometry>
       <mesh filename="package://jackal/meshes/link_2.dae"/>
      </geometry>
    </collision>
    <xacro:cylinder_inertia m="0.15" r="0.1" h="0.04"/>
</link>

<gazebo reference="link_rf">
    <static>0</static>
    <mu1>1</mu1>
    <mu2>1</mu2>
    <selfCollide>true</selfCollide> 
    <material>Gazebo/Grey</material>
</gazebo>

<joint name="joint_2" type="continuous">
    <axis xyz="0 1 0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="10" velocity="50"/>      
    <parent link="base_link"/>
    <child link="link_rf"/>
    <origin xyz="0.132 -0.190 -0.153"/>
</joint>

 <transmission name="right_front_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="right_front_wheel_motor">
    <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="joint_2">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

